<div class="container">
  <div class="well">
    <h2>My External Contacts</h2>
  </div>
  <div class="new-contact"></div> 
  <table class="table table-bordered table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Type</th>
        <th>Phone</th>
        <th>Title</th>
        <th>Ext</th>
        <th>Fax</th>
        <th>Email</th>
        <td>&nbsp;</td>
        <td>&nbsp;</td>
      </tr>
    </thead>
    <tbody class="contact-index">
      <%= render "index" %>
    </tbody>
  </table>
  <br>
  <div class="footer">
    <%= link_to "New Contact", new_contact_path, remote: true, class: "btn btn-primary" %>
  </div>
</div>

<div id="contact-modal" class="modal fade"></div>